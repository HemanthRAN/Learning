import java.util.Scanner;

public class MethodOverloadingCalculator13{

	
	 void calculate() {
		 Scanner scan=new Scanner(System.in);
	        System.out.println("Enter the first number: ");
	        int num1=scan.nextInt();
	        System.out.println("Enter the second number:");
	        int num2=scan.nextInt();
	        System.out.println("Enter the operator (+, -, *, /)");
	        String operator=scan.next();
	        scan.close();
		System.out.println(The result of addition of two number is : " + (num1+num2)); 
	}
	 void calculate() {
		 Scanner scan=new Scanner(System.in);
	        System.out.println("Enter the first number: ");
	        int num1=scan.nextInt();
	        System.out.println("Enter the second number:");
	        int num2=scan.nextInt();
	        System.out.println("Enter the operator (+, -, *, /)");
	        String operator=scan.next();
	        scan.close();
		System.out.println(The result of subraction of two number is : "+(num1-num2)); 
	}
	 void calculate() {
		 Scanner scan=new Scanner(System.in);
	        System.out.println("Enter the first number: ");
	        int num1=scan.nextInt();
	        System.out.println("Enter the second number:");
	        int num2=scan.nextInt();
	        System.out.println("Enter the operator (+, -, *, /)");
	        String operator=scan.next();
	        scan.close();
		System.out.println(The result of multiplication of two number is : "+(num1*num2)); 
	}
	 void calculate() {
		 Scanner scan=new Scanner(System.in);
	        System.out.println("Enter the first number: ");
	        int num1=scan.nextInt();
	        System.out.println("Enter the second number:");
	        int num2=scan.nextInt();
	        System.out.println("Enter the operator (+, -, *, /)");
	        String operator=scan.next();
	        scan.close();
		System.out.println(The result of division of two number is : "+(num1/num2)); 
	}

}
