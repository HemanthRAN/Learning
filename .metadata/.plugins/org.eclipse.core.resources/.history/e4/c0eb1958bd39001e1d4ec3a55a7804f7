import java.util.Scanner;

public class MethodOverloadingCalculator13 {

	public static void main(String[] args) {
        Scanner scan=new Scanner(System.in);
        System.out.println("Enter the first number: ");
        int num1=scan.nextInt();
        System.out.println("Enter the second number:");
        int num2=scan.nextInt();
        System.out.println("Enter the operator (+, -, *, /)");
        String operator=scan.next();
        scan.close();
        switch(operator) {
        case "+":
        	System.out.print(calculate( num1, num2));
        	break;
        case "-":
        	System.out.println(calculate( num1, num2));
        	break;
        case "*":
        	System.out.println(calculate( num1, num2));
        	break;
        case "/":
        	System.out.println(calculate( num1, num2));
        	break;
        default:
        	System.out.println("Invalid operator");
        	
        	
        }
        		
		

	}
	public static void calculate(int num1,int num2) {
		System.out.println("The result of addition of two numbers is :" +(num1+num2)); 
	}
	public static void calculate(int num1,int num2) {
		System.out.println("The result of addition of two numbers is :"+(num1-num2)); 
	}
	public static void calculate(int num1,int num2) {
		System.out.println("The result of addition of two numbers is :"+(num1*num2)); 
	}
	public static void calculate(int num1,int num2) {
		rSystem.out.println("The result of addition of two numbers is :"+(num1/num2)); 
	}

}
